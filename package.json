{
  "name": "react-flask-boilerplate",
  "version": "0.1.0",
  "private": true,
  "description": "React Flask Boilerplate. Uses React 16, Redux 4, Webpack 4, Python 3.7.1, Postgres 19 and Heroku. Support for multiple pages.",
  "scripts": {
    "gunicorn-dev": "gunicorn -b 127.0.0.1:5000 --reload --chdir server entry:app",
    "gunicorn": "gunicorn -b 0.0.0.0:$PORT --chdir server entry:app",
    "start": "webpack-dev-server --open --mode development",
    "dev": "concurrently --kill-others \"npm run start\" \"npm run gunicorn-dev\"",
    "test": "NODE_ENV=test BABEL_JEST_STAGE=0 jest",
    "dist": "NODE_ENV=production webpack",
    "postinstall": "npm run dist",
    "heroku-prebuild": "npm install --dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeremyletran/react-flask-boilerplate"
  },
  "engines": {
    "node": "6.15.1",
    "npm": "6.5.0"
  },
  "keywords": [
    "react",
    "webpack",
    "boilerplate",
    "production",
    "hot",
    "reload",
    "flask",
    "postgres",
    "redux"
  ],
  "author": "Jeremy Le-Tran",
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.24.1",
    "babel-runtime": "6.26.0",
    "bootstrap": "4.6.0",
    "concurrently": "4.1.0",
    "flummox": "3.6.8",
    "humps": "2.0.1",
    "immutability-helper": "2.9.0",
    "jquery": "3.6.0",
    "jss": "3.2.0",
    "jss-camel-case": "1.0.0",
    "jss-nested": "1.0.1",
    "jss-px": "1.0.0",
    "jss-vendor-prefixer": "1.0.1",
    "lodash": "4.17.21",
    "node-libs-browser": "1.0.0",
    "normalizr": "3.3.0",
    "promise": "7.1.1",
    "react": "16.7.0",
    "react-countdown-now": "1.3.0",
    "react-dom": "16.7.0",
    "react-jss": "2.0.1",
    "react-redux": "5.1.1",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-svg-loader": "2.1.0",
    "reactstrap": "6.5.0",
    "redux": "4.0.1",
    "redux-devtools": "3.5.0",
    "redux-devtools-dock-monitor": "1.1.3",
    "redux-devtools-log-monitor": "1.4.0",
    "redux-logger": "2.4.0",
    "redux-promise": "0.5.1",
    "redux-thunk": "2.3.0",
    "request": "2.88.0",
    "superagent-promise": "1.1.0",
    "whatwg-fetch": "0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "0.1.6",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "3.0.2",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "0.1.40",
    "jest-cli": "23.6.0",
    "jsx-loader": "0.13.2",
    "mini-css-extract-plugin": "^0.4.4",
    "mkdirp": "0.5.1",
    "prettier": "^1.13.7",
    "prettier-eslint": "^8.8.2",
    "react-scripts": "2.1.2",
    "react-transform-hmr": "1.0.1",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "3.11.2"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "testFileExtensions": [
      "test.js",
      "test.jsx"
    ],
    "testDirectoryName": "tests"
  }
}
